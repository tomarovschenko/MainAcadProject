    <TextField fx:id="tf_amount" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="3" />
    <Label id="lb_" fx:id="lb_number" font="$x1" text="Номер договора" GridPane.columnIndex="0" GridPane.rowIndex="0" />
    <Label id="lb_date" fx:id="lb_startDate" font="$x1" text="Дата договора" GridPane.columnIndex="0" GridPane.rowIndex="1" />
    <Label id="lb_endDate" fx:id="lb_amount" font="$x1" text="Сумма по договору" GridPane.columnIndex="0" GridPane.rowIndex="3" />
    <Label id="lb_date" fx:id="lb_endDate" font="$x1" text="Срок действия до" GridPane.columnIndex="0" GridPane.rowIndex="2" />
  </children>
