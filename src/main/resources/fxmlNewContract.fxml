        <Button fx:id="btn_cansel" font="$x1" mnemonicParsing="false" onAction="#cancel" text="Отмена" />
      </children>
    </HBox>
    <DatePicker fx:id="dp_startDate" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
