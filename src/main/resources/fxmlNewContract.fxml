<GridPane prefHeight="268.0" prefWidth="465.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="mainAcadProject.controller.NewContractController">
  <children>
    <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0" spacing="20.0" GridPane.columnIndex="1" GridPane.rowIndex="4">
      <children>
        <Button fx:id="btn_create" mnemonicParsing="false" onAction="#createContract" text="Внести">
          <font>
            <Font size="14.0" fx:id="x1" />
          </font>
