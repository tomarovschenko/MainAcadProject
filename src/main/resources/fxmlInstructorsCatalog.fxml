<GridPane prefHeight="291.0000999999975" prefWidth="1350.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="mainAcadProject.controller.InstructorsCatalogController">
  <children>
    <TableView fx:id="instructors_catalog" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="0" GridPane.rowIndex="0">
      <columns>
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="217.0" text="Фамилия" fx:id="instructor_surname" />
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="141.0" text="Имя" fx:id="instructor_name" />
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="155.0" text="Отчество" fx:id="instructor_patronymic" />
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="164.0" text="Контактный телефон" fx:id="instructor_phone" />
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="103.0" text="Паспорт" fx:id="instructor_passport" />
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="196.0" text="e-mail" fx:id="instructor_mail" />
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="355.0" text="Актуальное место работы" fx:id="instructor_working" />
      </columns>
    </TableView>
    <HBox prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="0" GridPane.rowIndex="1">
      <children>
        <HBox alignment="CENTER_LEFT" prefHeight="45.0" prefWidth="547.0" spacing="20.0">
          <children>
            <Button id="btn_" fx:id="btn_edit" mnemonicParsing="false" onAction="#editInstructor" text="Редактировать">
              <font>
                <Font size="14.0" fx:id="x1" />
              </font>
            </Button>
            <Button fx:id="btn_delete" font="$x1" mnemonicParsing="false" onAction="#delete" text="Удалить" />
          </children>
        </HBox>
        <HBox alignment="CENTER_RIGHT" prefHeight="45.0" prefWidth="783.0">
          <children>
            <Button fx:id="btn_close" font="$x1" mnemonicParsing="false" onAction="#close" text="Закрыть" textAlignment="CENTER" />
          </children>
        </HBox>
      </children>
    </HBox>
  </children>
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
  </columnConstraints>
  <padding>
    <Insets left="10.0" right="10.0" top="10.0" />
  </padding>
  <rowConstraints>
    <RowConstraints maxHeight="236.0" minHeight="10.0" prefHeight="226.0" vgrow="SOMETIMES" />
    <RowConstraints maxHeight="52.0" minHeight="0.0" prefHeight="30.0" vgrow="SOMETIMES" />
  </rowConstraints>
