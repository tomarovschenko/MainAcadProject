<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.GridPane?>

<GridPane alignment="CENTER" hgap="10.0" vgap="10.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="mainAcadProject.controller.CourseController">
  <children>
      <AnchorPane prefHeight="550.0" prefWidth="700.0" GridPane.columnIndex="0" GridPane.rowIndex="0">
          <children>
              <TableView fx:id="table_courses" layoutX="0.0" layoutY="0.0" prefHeight="481.0" prefWidth="688.0">
                  <columns>
                      <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="444.0" text="Наименование курса" fx:id="tbl_col_name" />
                      <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="240.0" text="Кол-во учебных часов" fx:id="tbl_col_hours" />
                  </columns>
              </TableView>
              <Button fx:id="btn_cancel" layoutX="504.0" layoutY="507.0" mnemonicParsing="false" text="Отмена" />
              <Button fx:id="btn_add" layoutX="289.0" layoutY="507.0" mnemonicParsing="false" onAction="#add" text="Добавить" />
              <Button fx:id="btn_save" layoutX="395.0" layoutY="507.0" mnemonicParsing="false" onAction="#saveСhanges" text="Сохранить" />
              <Button fx:id="btn_del" layoutX="601.0" layoutY="507.0" mnemonicParsing="false" onAction="#delete" text="Удалить" />
          </children>
          <padding>
              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" fx:id="x1" />
          </padding>
      </AnchorPane>
  </children>
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
  </rowConstraints>
</GridPane>
